{
  "frontend/src/components/AgentDashboard.vue": {
    "target": "backend/ui/agent_dashboard_bridge.py",
    "binds": [
      "configs/agent_registry.json",
      ".github/workflows/multi-agent-issue-coordinator.yml"
    ],
    "integration_type": "ui_component",
    "requires_api": true
  },
  "frontend/src/components/AgentStatusPanel.vue": {
    "target": "backend/ui/status_panel_bridge.py",
    "binds": [
      "configs/agent_registry.json"
    ],
    "integration_type": "ui_component",
    "requires_api": false
  },
  "frontend/src/components/AssimilationMonitor.vue": {
    "target": "backend/ui/assimilation_monitor_bridge.py",
    "binds": [
      "configs/frontend_manifest.json"
    ],
    "integration_type": "ui_component",
    "requires_api": true
  },
  "frontend/src/utils/agentApi.js": {
    "target": "backend/api/agent_api_bridge.py",
    "binds": [
      "configs/agent_registry.json",
      "backend/services/agent_core_bridge.py"
    ],
    "integration_type": "api_client",
    "requires_api": true
  },
  "frontend/src/utils/assimilationClient.js": {
    "target": "backend/api/assimilation_client_bridge.py",
    "binds": [
      "configs/frontend_manifest.json",
      "configs/frontend_to_backend_map.json",
      "tools/assimilation_audit.py"
    ],
    "integration_type": "api_client",
    "requires_api": true
  },
  "frontend/src/utils/forensicLogger.js": {
    "target": "backend/utils/forensic_logger_bridge.py",
    "binds": [
      "logs/activation.jsonl",
      "configs/activation_rules.json"
    ],
    "integration_type": "utility",
    "requires_api": false
  },
  "frontend/src/services/githubIntegration.js": {
    "target": "backend/services/github_integration_bridge.py",
    "binds": [
      "ai/bridges/github_bridge.py",
      "configs/agent_registry.json",
      ".github/workflows/"
    ],
    "integration_type": "service",
    "requires_api": true
  },
  "frontend/src/services/workflowRunner.js": {
    "target": "backend/services/workflow_runner_bridge.py",
    "binds": [
      ".github/workflows/agent-command-router.yml",
      ".github/workflows/pr-assimilation-audit.yml",
      "configs/activation_rules.json"
    ],
    "integration_type": "service",
    "requires_api": true
  },
  "tools/living-code-coreV2!!.js": {
    "target": "ai/living_core.py",
    "binds": [
      "configs/activation_rules.json", 
      "ai/agent_core.py",
      "logs/activation.jsonl"
    ],
    "integration_type": "core_system",
    "requires_api": false
  },
  "tools/providers.js": {
    "target": "ai/providers.py",
    "binds": [
      "configs/agent_registry.json", 
      "ai/agent.py",
      "backend/services/integration_engine.py"
    ],
    "integration_type": "provider",
    "requires_api": false
  },
  "tools/server.js": {
    "target": "backend/server.py",
    "binds": [
      "backend/api/agent_endpoints.py",
      "backend/api/assimilation_api.py", 
      "scripts/",
      "ai/"
    ],
    "integration_type": "server",
    "requires_api": true
  },
  "tools/assimilation_audit.py": {
    "target": "backend/validation/assimilation_validator_bridge.py",
    "binds": [
      "configs/frontend_manifest.json", 
      "configs/frontend_to_backend_map.json",
      ".github/workflows/pr-assimilation-audit.yml"
    ],
    "integration_type": "validation",
    "requires_api": false
  },
  "ai/agent.py": {
    "target": "backend/services/agent_core_bridge.py",
    "binds": [
      "configs/agent_registry.json", 
      "ai/agent_core.py",
      "backend/services/integration_engine.py"
    ],
    "integration_type": "ai_core",
    "requires_api": false
  },
  "ai/agent_core.py": {
    "target": "backend/services/core_engine_bridge.py",
    "binds": [
      "configs/activation_rules.json", 
      "ai/living_core.py",
      "logs/activation.jsonl"
    ],
    "integration_type": "ai_core",
    "requires_api": false
  },
  "ai/bridges/github_bridge.py": {
    "target": "backend/bridges/github_bridge_adapter.py",
    "binds": [
      "configs/agent_registry.json", 
      ".github/workflows/",
      "frontend/src/services/githubIntegration.js"
    ],
    "integration_type": "bridge",
    "requires_api": true
  },
  "ai/bridges/workflow_bridge.py": {
    "target": "backend/bridges/workflow_bridge_adapter.py",
    "binds": [
      ".github/workflows/agent-command-router.yml",
      "configs/activation_rules.json",
      "frontend/src/services/workflowRunner.js"
    ],
    "integration_type": "bridge",
    "requires_api": true
  },
  "backend/api/agent_endpoints.py": {
    "target": "backend/api/agent_endpoints_bridge.py",
    "binds": [
      "backend/server.py",
      "ai/agent_core.py",
      "frontend/src/utils/agentApi.js"
    ],
    "integration_type": "api_endpoint",
    "requires_api": true
  },
  "backend/api/assimilation_api.py": {
    "target": "backend/api/assimilation_api_bridge.py",
    "binds": [
      "tools/assimilation_audit.py",
      "configs/frontend_manifest.json",
      "frontend/src/utils/assimilationClient.js"
    ],
    "integration_type": "api_endpoint",
    "requires_api": true
  },
  "backend/services/integration_engine.py": {
    "target": "backend/services/integration_engine_bridge.py",
    "binds": [
      "ai/agent_core.py",
      "tools/assimilation_audit.py",
      "configs/frontend_to_backend_map.json"
    ],
    "integration_type": "core_service",
    "requires_api": false
  },
  "backend/services/forensic_service.py": {
    "target": "backend/services/forensic_service_bridge.py",
    "binds": [
      "logs/activation.jsonl",
      "logs/assimilation.jsonl",
      "frontend/src/utils/forensicLogger.js"
    ],
    "integration_type": "core_service",
    "requires_api": false
  }
}